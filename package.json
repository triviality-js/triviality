{
  "name": "triviality",
  "private": false,
  "version": "0.3.0",
  "description": "Purely typed service container",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "prepublish": "yarn create-docs && yarn test && yarn create-build",
    "watch": "tsc --watch",
    "test": "npm-run-all --parallel jest-coverage tslint jscpd tsc",
    "jest": "jest",
    "tsc": "tsc",
    "stryker": "stryker run",
    "jest-coverage": "jest --coverage",
    "tslint-fix": "tslint --fix --project tsconfig.json",
    "tslint": "tslint --project tsconfig.json",
    "jscpd": "jscpd --limit=-1",
    "jest-badges": "yarn jest-coverage && jest-coverage-badges",
    "create-container-types": "ts-node src/cli/createContainerTypes.ts",
    "create-docs": "node docs/build.js",
    "create-build": "rm -fr build && yarn tsc --project tsconfig.build.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epinxteren/triviality.git"
  },
  "keywords": [
    "typescript",
    "service container",
    "di",
    "dependency injection"
  ],
  "author": "Eric Pinxteren",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/epinxteren/triviality/issues"
  },
  "homepage": "https://github.com/epinxteren/triviality#README",
  "devDependencies": {
    "@types/jest": "^22.1.0",
    "@types/node": "^9.3.0",
    "jest": "^23.5.0",
    "jest-coverage-badges": "^1.0.0",
    "jscpd": "^0.6.17",
    "markdown-include": "^0.4.3",
    "npm-run-all": "^4.1.3",
    "stryker": "^0.29.2",
    "stryker-api": "^0.21.1",
    "stryker-html-reporter": "^0.16.2",
    "stryker-jest-runner": "^1.2.1",
    "stryker-typescript": "^0.14.1",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-loader": "^3.5.3",
    "tslint-microsoft-contrib": "^5.0.2",
    "typescript": "^3.2",
    "child-process-promise": "^2.2.1"
  },
  "dependencies": {
    "@types/lodash.memoize": "^4.1.4",
    "lodash.memoize": "^4.1.2"
  }
}
